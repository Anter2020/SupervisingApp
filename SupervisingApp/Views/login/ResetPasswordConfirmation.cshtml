
@{
    ViewBag.Title = "ResetPasswordConfirmation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<hgroup class="title">
    <h1 style="color:white">@ViewBag.Title.</h1>
</hgroup>
<div>
    <p style="color:white">
        Your password has been reset. Please @Html.ActionLink("click here to log in", "register", "login", routeValues: null, htmlAttributes: new { id = "loginLink" })
    </p>
</div>
